{
    "name": "wasksofts/omnipay-lipa-na-mpesa",
    "description": ":package_description",
    "keywords": [
        "omnipay",
        "lipa-na-mpesa"
    ],
    "homepage": "https://github.com/:vendor/omnipay-lipa-na-mpesa",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "mukamanusteven@gmail.com",
            "homepage": "https://krnyadevelopers.co.ke"
        }
    ],
    "require": {
        "omnipay/common": "v3.0-alpha.2"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Mpesa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Mpesa\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
